{
  "id": "7e5c5afe-ae23-45ad-8668-6be49b98533d",
  "type": "authentication-assurance",
  "external_service": "mocky",
  "external_application_id_param": "application_id",
  "processes": {
    "apply": {
      "url": "http://localhost:4000/apply",
      "method": "POST",
      "headers": {
        "Content-Type": "application/json",
        "Authorization": "Bearer dummy-token"
      },
      "dynamic_body_keys": [
        "last_name",
        "first_name",
        "last_name_kana",
        "last_name_kana",
        "first_name_kana",
        "birthdate",
        "nationality",
        "email_address",
        "mobile_phone_number",
        "address"
      ],
      "static_body": {},
      "request_validation_schema": {
        "$schema": "https://json-schema.org/draft/2020-12/schema",
        "title": "request Application",
        "type": "object",
        "required": [
          "last_name",
          "first_name",
          "last_name_kana",
          "first_name_kana",
          "birthdate",
          "nationality",
          "email_address",
          "mobile_phone_number",
          "address"
        ],
        "properties": {
          "last_name": {
            "type": "string",
            "maxLength": 255,
            "store": true
          },
          "first_name": {
            "type": "string",
            "maxLength": 255,
            "store": true
          },
          "last_name_kana": {
            "type": "string",
            "maxLength": 255,
            "store": true
          },
          "first_name_kana": {
            "type": "string",
            "maxLength": 255,
            "store": true
          },
          "birthdate": {
            "type": "string",
            "format": "date",
            "store": true
          },
          "nationality": {
            "type": "string",
            "maxLength": 255,
            "store": true
          },
          "email_address": {
            "type": "string",
            "maxLength": 255,
            "pattern": "^[\\w\\.-]+@[\\w\\.-]+\\.[a-zA-Z]{2,}$",
            "store": true
          },
          "mobile_phone_number": {
            "type": "string",
            "maxLength": 11,
            "pattern": "^[0-9]{10,11}$",
            "store": true
          },
          "address": {
            "type": "object",
            "store": true,
            "properties": {
              "street_address": {
                "type": "string",
                "maxLength": 255
              },
              "locality": {
                "type": "string",
                "maxLength": 255
              },
              "region": {
                "type": "string",
                "maxLength": 255
              },
              "postal_code": {
                "type": "string",
                "maxLength": 255
              },
              "country": {
                "type": "string",
                "maxLength": 255
              }
            },
            "additionalProperties": false
          }
        },
        "additionalProperties": false
      },
      "request_additional_parameter_schema": {
        "$schema": "original",
        "user": {
          "mapping_rules": [
            { "from": "$.sub", "to": "user_id" },
            { "from": "$.email", "to": "mail_address" },
            { "from": "$.custom_properties.token", "to": "token" }
          ]
        },
        "http_request": {
          "url": "http://localhost:4000/apply",
          "method": "POST",
          "headers": {
            "Content-Type": "application/json",
            "Authorization": "Bearer dummy-token"
          },
          "dynamic_body_keys": [
            "last_name",
            "first_name",
            "last_name_kana",
            "first_name_kana"
          ],
          "response_validation_schema": {
            "$schema": "https://json-schema.org/draft/2020-12/schema",
            "title": "external user data response",
            "type": "object",
            "required": [
              "application_id"
            ],
            "properties": {
              "application_id": {
                "type": "string",
                "store": true
              }
            },
            "additionalProperties": false
          }
        }
      },
      "request_verification_schema": {
        "$schema": "original",
        "duplicate_application": false,
        "unmatched_user_claims_email": {
          "property": "email_address"
        },
        "unmatched_user_claims_phone": {
          "property": "mobile_phone_number"
        }
      },
      "response_validation_schema": {
        "$schema": "https://json-schema.org/draft/2020-12/schema",
        "title": "application response",
        "type": "object",
        "required": [
          "application_id"
        ],
        "properties": {
          "application_id": {
            "type": "string",
            "store": true
          }
        },
        "additionalProperties": false
      }
    },
    "request-authentication": {
      "url": "http://localhost:4000/request-authentication",
      "method": "POST",
      "headers": {
        "Content-Type": "application/json",
        "Authorization": "Bearer dummy-token"
      },
      "dynamic_body_keys": [
        "trust_framework",
        "evidence_document_type"
      ],
      "static_body": {},
      "request_validation_schema": {
        "$schema": "https://json-schema.org/draft/2020-12/schema",
        "title": "request eKYC",
        "type": "object",
        "required": [
          "trust_framework",
          "evidence_document_type"
        ],
        "properties": {
          "trust_framework": {
            "type": "string",
            "maxLength": 100,
            "store": true
          },
          "evidence_document_type": {
            "type": "string",
            "maxLength": 50,
            "store": true
          }
        },
        "additionalProperties": false
      },
      "response_validation_schema": {
        "$schema": "https://json-schema.org/draft/2020-12/schema",
        "title": "tx response",
        "type": "object",
        "required": [
          "tx_id"
        ],
        "properties": {
          "tx_id": {
            "type": "string",
            "respond": true,
            "store": true
          }
        },
        "additionalProperties": false
      }
    },
    "authentication-status": {
      "url": "http://localhost:4000/authentication-status",
      "method": "POST",
      "headers": {
        "Content-Type": "application/json",
        "Authorization": "Bearer dummy-token"
      },
      "dynamic_body_keys": [],
      "static_body": {},
      "request_validation_schema": {
        "$schema": "https://json-schema.org/draft/2020-12/schema",
        "title": "complete eKYC",
        "type": "object",
        "required": [],
        "properties": {},
        "additionalProperties": false
      },
      "response_validation_schema": {
        "$schema": "https://json-schema.org/draft/2020-12/schema",
        "title": "complete eKYC response",
        "type": "object",
        "required": [],
        "properties": {},
        "additionalProperties": false
      }
    },
    "evaluate-result": {
      "description": "",
      "completion_condition_configuration": {
        "request_condition": {
          "all_of": [
            {
              "path": "$.approved",
              "type": "boolean",
              "operation": "eq",
              "value": true
            },
            {
              "path": "$.rejected",
              "type": "boolean",
              "operation": "eq",
              "value": false
            }
          ]
        },
        "process_condition": {
          "all_of": [
            {
              "type": "request-authentication",
              "success_count": 1
            },
            {
              "type": "authentication-status",
              "success_count": 1
            }
          ]
        }
      },
      "rejected_condition_configuration": {
        "description": "",
        "request_condition": {
          "all_of": [
            {
              "source": "request_body",
              "path": "$.approved",
              "operation": "eq",
              "value": false
            },
            {
              "source": "request_body",
              "path": "$.rejected",
              "operation": "eq",
              "value": true
            }
          ]
        }
      }
    }
  }
}