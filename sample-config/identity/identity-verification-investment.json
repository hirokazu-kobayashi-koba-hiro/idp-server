        "required": [],
        "properties": {},
        "additionalProperties": false
      },
      "response_validation_schema": {
        "$schema": "https://json-schema.org/draft/2020-12/schema",
        "title": "complete eKYC response",
        "type": "object",
        "required": [],
        "properties": {},
        "additionalProperties": false
      }
    }
  }
}
