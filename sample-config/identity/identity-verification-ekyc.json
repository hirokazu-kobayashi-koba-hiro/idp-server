          "last_name_kana": { "type": "string", "maxLength": 255 },
          "first_name_kana": { "type": "string", "maxLength": 255 },
          "birthdate": { "type": "string", "format": "date" },
          "nationality": { "type": "string", "maxLength": 255 },
