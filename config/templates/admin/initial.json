  "user": {
    "sub": "${ADMIN_USER_SUB}",
    "provider_id": "idp-server",
    "name": "${ADMIN_USER_EMAIL}",
    "email": "${ADMIN_USER_EMAIL}",
    "email_verified": true,
    "raw_password": "${ADMIN_USER_PASSWORD}",
    "role": "Administrator"
  },
  "client": {
    "client_id": "${ADMIN_CLIENT_ID}",
    "client_id_alias": "${ADMIN_CLIENT_ID_ALIAS}",
    "client_secret": "${ADMIN_CLIENT_SECRET}",
    "redirect_uris": [
